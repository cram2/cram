<package format="2">
  <name>spatial_relations_demo</name>
  <version>0.6.0</version>
  <description>Some Lisp code for using CRAM in projection and showcasing location designators</description>
  <author>Gayane Kazhoyan</author>
  <maintainer email="kazhoyan@cs.uni-bremen.de">Gayane Kazhoyan</maintainer>
  <license>BSD</license>

  <url type="website">http://cram-system.org</url>
  <url type="repository">http://github.com/cram2/cram_projection_demos</url>

  <buildtool_depend>catkin</buildtool_depend>
  <depend>cram_bullet_reasoning_costmap</depend>
  <depend>cram_tf</depend>
  <depend>cram_designators</depend>
  <depend>cram_location_costmap</depend>
  <depend>cram_semantic_map_costmap</depend>
  <depend>cram_robot_pose_gaussian_costmap</depend>
  <depend>cram_bullet_reasoning</depend>
  <depend>cram_pr2_description</depend>
  <depend>cram_bullet_reasoning_belief_state</depend>
  <depend>cram_pr2_synch_projection_pms</depend>
  <depend>cram_occupancy_grid_costmap</depend>
  <depend>cram_plan_library</depend>
  <depend>cram_bullet_reasoning_designators</depend>
  <depend>cram_common_designators</depend>
  <depend>cram_semantic_map_designators</depend>
  <depend>alexandria</depend>
  <depend>cram_bullet_reasoning_utilities</depend>
  <depend>pr2_reachability_costmap</depend>

  <!-- launch file -->
  <exec_depend>iai_maps</exec_depend>
  <exec_depend>map_server</exec_depend>
  <exec_depend>pr2_description_lowres</exec_depend>
  <exec_depend>pr2_moveit_config</exec_depend>
  <exec_depend>pr2_moveit_plugins</exec_depend>
</package>
