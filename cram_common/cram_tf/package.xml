<package format="2">
  <name>cram_tf</name>
  <version>0.7.0</version>
  <description>
    This package supplies application specific functionalities for
    designators with in the ROS system.
  </description>

  <author>Lorenz Moesenlechner</author>

  <maintainer email="kazhoyan@cs.uni-bremen.de">Gayane Kazhoyan</maintainer>

  <license>BSD</license>

  <url type="website">http://cram-system.org</url>
  <url type="bugtracker">https://github.com/cram2/cram_plans/issues</url>
